<mvc:View controllerName="zprojectodatae1102.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:t="sap.ui.table"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
        <t:Table
				rows="{/CarrierSet}"
				selectionMode="MultiToggle"
				visibleRowCount="7"
				paste="onPaste"
				ariaLabelledBy="title">
				<t:extension>
					<OverflowToolbar style="Clear">
						<Title id="title" text="Products"/>
					</OverflowToolbar>
				</t:extension>
				<t:columns>
					<t:Column >
						<Label text="Carrid" />
						<t:template>
							<Text text="{Carrid}" wrapping="false" /> 
						</t:template>
					</t:Column>
                    <t:Column>
						<Label text="Carrname" />
						<t:template>
							<Text text="{Carrname}" wrapping="false" />
						</t:template>
					</t:Column>
                    <t:Column>
						<Label text="Currcode" />
						<t:template>
							<Text text="{Currcode}" wrapping="false" />
						</t:template>
					</t:Column>
				</t:columns>
			</t:Table>
        </content>
    </Page>
</mvc:View>
